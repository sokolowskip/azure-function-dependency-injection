<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Willezone.Azure.WebJobs.Extensions.DependencyInjection</RootNamespace>
    <AssemblyName>Willezone.Azure.WebJobs.Extensions.DependencyInjection</AssemblyName>
    <Authors>Boris Wilhelms</Authors>
    <Company>Boris Wilhelms</Company>
    <Description>This package contains binding extensions for dependency injection.</Description>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Azure WebJobs AzureFunctions DependencyInjection</PackageTags>
    <RepositoryUrl>https://github.com/BorisWilhelms/azure-function-dependency-injection</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/BorisWilhelms/azure-function-dependency-injection</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/BorisWilhelms/azure-function-dependency-injection/blob/master/LICENSE</PackageLicenseUrl>
    <AssemblyVersion>1.0.1.0</AssemblyVersion>
    <FileVersion>1.0.1.0</FileVersion>
    <Version>1.0.1</Version>
    <LangVersion>7.3</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.WebJobs" Version="3.0.4" />
    <PackageReference Include="Microsoft.Azure.WebJobs.Extensions.ServiceBus" Version="3.0.3" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="2.2.0" />
  </ItemGroup>
</Project>
